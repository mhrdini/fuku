{
  "name": "@fuku/database",
  "version": "1.0.0",
  "exports": {
    ".": "./src/client.ts"
  },
  "scripts": {
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:dev:auto": "prisma migrate dev --name auto",
    "db:push": "prisma db push",
    "db:seed": "tsx src/seed.ts",
    "db:seed:dev": "tsx src/seed.development.ts",
