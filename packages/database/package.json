    "predev": "npm run generate",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "latest"
  },
  "devDependencies": {
    "@fuku/config-eslint": "workspace:*",
    "@fuku/config-typescript": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prisma": "latest",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
