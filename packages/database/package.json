    "@types/node": "^20.19.23",
    "eslint": "^9.38.0",
    "prisma": "latest",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "tsx": "4.19.1",
    "typescript": "5.5.4"
  }
