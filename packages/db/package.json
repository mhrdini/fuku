{
  "name": "@fuku/db",
  "version": "1.0.0",
  "exports": {
    ".": "./src/client.ts"
  },
  "prisma": {
    "schema": "./prisma"
  },
  "scripts": {
    "dev": "prisma generate",
    "gen:all": "prisma generate",
    "db:migrate:deploy": "prisma migrate deploy && prisma generate",
    "db:migrate:dev": "prisma migrate dev && prisma generate",
    "db:migrate:dev:auto": "prisma migrate dev --name auto && prisma generate",
    "db:push": "prisma db push && prisma generate",
    "db:seed": "tsx src/seed.ts",
    "db:seed:dev": "tsx src/seed.development.ts",
    "db:reset": "prisma migrate reset",
    "format": "prisma format",
    "lint": "eslint . --max-warnings 0",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/adapter-pg": "catalog:",
    "@prisma/client": "catalog:"
  },
  "devDependencies": {
    "@fuku/config-eslint": "workspace:*",
    "@fuku/config-typescript": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prisma": "catalog:",
    "rimraf": "^5.0.5",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
