{
  "name": "@fuku/db",
  "version": "1.0.0",
  "exports": {
    ".": "./src/client.ts",
    "./schemas": "./generated/schemas/index.ts"
  },
  "prisma": {
    "schema": "./prisma"
  },
  "scripts": {
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev --skip-generate",
    "db:migrate:dev:auto": "prisma migrate dev --name auto --skip-generate",
    "db:push": "prisma db push --skip-generate",
    "db:seed": "tsx src/seed.ts",
    "db:seed:dev": "tsx src/seed.development.ts",
    "db:reset": "prisma migrate reset --skip-generate",
    "dev": "tsup",
    "format": "prisma format",
    "gen:all": "prisma generate",
    "gen:client": "prisma generate",
    "gen:schemas": "prisma generate --generator prisma-zod-generator",
    "lint": "eslint . --max-warnings 0",
    "prebuild": "npm run gen:all",
    "predev": "npm run gen:all",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "6.19.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@fuku/config-eslint": "workspace:*",
    "@fuku/config-typescript": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prisma": "6.19.0",
    "prisma-zod-generator": "1.31.2",
    "rimraf": "^5.0.5",
    "tsup": "^8.5.1",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
